{
    "name": "torchlight/laravel",
    "description": "A Laravel Client for Torchlight, the syntax highlighting API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Francis",
            "email": "aaron@hammerstone.dev"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/console": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "mockery/mockery": "^1.3.3",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Hammerstone\\Torchlight\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hammerstone\\Torchlight\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hammerstone\\Torchlight\\TorchlightServiceProvider"
            ]
        }
    }
}
